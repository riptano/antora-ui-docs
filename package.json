{
  "name": "landing-pages",
  "scripts": {
    "build:prod:landing-pages": "npm run build:publish:landing-pages",
    "build:dev:landing-pages": "npm run build:local:landing-pages",
    "build:publish:landing-pages": "antora --fetch --clean playbooks/site-publish-landing-pages.yaml",
    "build:local:landing-pages": "antora --stacktrace --fetch --clean playbooks/site-local-landing-pages.yaml",
    "build:linkinator": "npx linkinator https://docs.datastax.com/en/home/docs --recurse --skip ' http://localhost?'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riptano/guide-docs.git"
  },
  "dependencies": {
    "@antora/cli": "^3.0.1",
    "@antora/site-generator-default": "^3.0.1",
    "linkinator": "^3.0.3",
    "async": "3.2.4",
    "mobx": "^6.0.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "rxjs": "7.0.1",
    "styled-components": "^5.1.1"
  }
}
